[
  {
    "essential": true,
    "portMappings": [
    ],
    "mountPoints": [
      {
        "containerPath": "/data",
        "sourceVolume": "data"
      }
    ],
    "name": "gogs",
    "environment": [
    ],
    "image": "gogs/gogs:latest",
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "${log_group_name}",
        "awslogs-region": "us-east-1",
        "awslogs-stream-prefix": "gogs"
      }
    },
    "memoryReservation": 100
  }
]
