[
  {
    "essential": true,
    "portMappings": [
    ],
    "mountPoints": [
      {
        "containerPath": "/srv/gitlab/config",
        "sourceVolume": "config"
      },
      {
        "containerPath": "/srv/gitlab/data",
        "sourceVolume": "data"
      }
    ],
    "name": "gitlab",
    "environment": [
      {
        "name": "GITLAB_OMNIBUS_CONFIG",
        "value": "external_url \"http://gitlab:8080\""
      }
    ],
    "image": "gitlab/gitlab-ce:latest",
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "${log_group_name}",
        "awslogs-region": "us-east-1",
        "awslogs-stream-prefix": "gitlab"
      }
    },
    "memoryReservation": 200
  }
]
